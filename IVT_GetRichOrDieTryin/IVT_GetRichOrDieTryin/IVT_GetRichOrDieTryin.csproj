  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.ML" Version="1.6.0" />
    <PackageReference Include="Microsoft.ML.FastTree" Version="1.6.0" />
  </ItemGroup>
  <ItemGroup Label="ML-Favorite">
    <None Include="ML-Favorite.consumption.cs">
      <DependentUpon>ML-Favorite.mbconfig</DependentUpon>
    </None>
    <None Include="ML-Favorite.training.cs">
      <DependentUpon>ML-Favorite.mbconfig</DependentUpon>
    </None>
    <None Include="ML-Favorite.zip">
      <DependentUpon>ML-Favorite.mbconfig</DependentUpon>
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
  </ItemGroup>
</Project>